<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cz.mendelu.busitweek_app1">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".DigActivity"
            android:label="@string/title_activity_dig"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".DominatedActivity"
            android:label="@string/title_activity_dominated"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PeeActivity"
            android:label="@string/title_activity_pee"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BarkFailActivity"
            android:label="@string/title_activity_bark_fail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MeasureActivity"
            android:label="@string/title_activity_measure"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BarkActivity"
            android:label="@string/title_activity_bark"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DeadActivity"
            android:label="@string/title_activity_dead"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".WildCatActivity"
            android:label="@string/title_activity_wild_cat"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LostCatActivity"
            android:label="@string/title_activity_lost_cat"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CaughtCatActivity"
            android:label="@string/title_activity_caught_cat"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EscapeActivity"
            android:label="@string/title_activity_escape"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ReunionActivity"
            android:label="@string/title_activity_reunion"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MapActivity"
            android:label="@string/title_activity_map"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SimplePuzzleActivity"
            android:label="@string/title_activity_simple_puzzle"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ChoicePuzzleActivity"
            android:label="@string/title_activity_choice_puzzle"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ImagePuzzleActivity"
            android:label="@string/title_activity_image_puzzle"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FinishActivity"
            android:label="@string/title_activity_finish"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".IntroActivity"
            android:label="@string/title_activity_intro"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>